<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res/tw.jouou.aRoundTable"
	android:orientation="vertical" android:layout_width="fill_parent"
	android:layout_height="fill_parent">
		<LinearLayout android:layout_height="wrap_content"
		android:layout_width="wrap_content"  android:layout_alignParentRight="true"
		android:id="@+id/header_layout">
		<org.taptwo.android.widget.CircleFlowIndicator
			android:padding="10dip" android:layout_height="wrap_content"
			android:layout_width="wrap_content" android:id="@+id/viewflowindic2" android:background="#00000000"/>
	</LinearLayout>
	<org.taptwo.android.widget.ViewFlow
		android:id="@+id/viewflow" android:layout_width="fill_parent"
		android:layout_height="fill_parent" app:sidebuffer="4"/>
</RelativeLayout> 
